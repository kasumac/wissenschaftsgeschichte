@Incollection{kassung:2017c,
  author    = {Christian Kassung},
  title     = {Reisezeit -- Erzählzeit -- Raumzeit. Nichtlineares Erzählen bei Stanislaw Lem},
  booktitle = {Science oder Fiction? Stanislaw Lems Philosophie der Wissenschaft und Technik},
  publisher = {Wilhelm Fink Verlag},
  year      = {2017},
  mode      = {Aufsatz},
